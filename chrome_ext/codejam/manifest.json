{
  "name": "CodeJam",
  "version": "0.0.0.1",    
  "description": "A small help with code jam",
  "page_action": {
    "default_icon": "resources/icon_off.jpg",
    "default_title": "Code Jam"    
  },
  "options_page": "options/options.html",
  "content_scripts": [
    {
	  "matches": ["https://code.google.com/codejam/*"],	  
	  "css": ["content/content.css"],
      "js": [
		"resources/jquery.js",
		"content/messageListenner.js",
		"content/extensionState.js",
		"content/pageManagement.js",
		"content/problemSolver.js",
		"content/content.js"
	  ],
	  "run_at": "document_idle"
    }
  ],   
  "permissions": [
    "activeTab",
	"https://code.google.com/codejam/contest/*",
	"tabs"
  ],
  "background": {
    "scripts": ["background/background.js"],
	"persistent": false
  }, 
  "manifest_version": 2
}
